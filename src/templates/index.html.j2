                  <span class="icon">
                    <svg width="16" height="16" version="2.0" class="bi bi-plus-circle">
                      <use href="#circle-arrow-plus" />
                    </svg>
                    <svg width="16" height="16" version="2.0" class="bi bi-dash-circle">
                      <use href="#circle-arrow-minus" />
                    </svg>
                  </span>
