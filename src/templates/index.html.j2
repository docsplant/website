                <div class="accordion-body">
                  <p>
                    {{ dac_section.elements[2].description }}
                  </p>
                </div>
              </div>
            </div>
          </div>
        </div>
      </section>
      <!-- FAQ Section End -->

      <!-- Contributing Section Start -->
      <section class="contributing-seaction section">
        <div class="container">
          <div class="contributing-quote">
            <div class="quote-img">
              <svg width="84" height="84" viewBox="0 0 84 84" fill="none" xmlns="http://www.w3.org/2000/svg">
                <use href="#quote_symbol" />
              </svg>
            </div>
            <div class="quote-title">
              <h3>
                {{ quote_section[0].quote }}
              </h3>
              <p>
                {{ quote_section[0].description }}
              </p>
            </div>
          </div>
          <div class="contributing-img">
            <img src="../assets/images/docsplan_logo_plain.svg" class="normal_img">
            <img src="../assets/images/docsplan_logo_plain_dark.svg" class="white_img">
          </div>
          <div class="contributing-quote contributing-two">
            <div class="quote-img">
              <svg width="84" height="84" viewBox="0 0 84 84" fill="none" xmlns="http://www.w3.org/2000/svg">
                <use href="#quote_symbol" />
              </svg>
            </div>
            <div class="quote-title">
              <h3>
                {{ quote_section[1].quote }}
              </h3>
              <p>
                {{ quote_section[1].description }}
              </p>
            </div>
          </div>
        </div>
      </section>
      <!-- Contributing Seaction End -->

      <!-- Timeline Seaction Start -->
      <section class="timeline-section section">
        <div class="container">
          <div class="timeline-header">
            <h2>{{ timeline_section.title }}</h2>
          </div>
          <!-- single-timeline -->
          <div class="single-timeline analysis timeline-padding">
            <div class="timeline-img timeline-img-1">
              <img src="../assets/images/silene_dioica_seed.svg" class="normal_img">
              <img src="../assets/images/silene_dioica_seed_dark.svg" class="white_img">
              <span class="arrow_icon">
                <svg width="101" height="71">
                  <use href="#arrow_symbol" />
                </svg>
              </span>
            </div>
            <div class="timeline-text">
              <h2>{{ timeline_section.elements[0].title }}</h2>
              <p>
                {{ timeline_section.elements[0].description }}
              </p>
            </div>
          </div>
          <!-- single-timeline -->
          <div class="single-timeline win-timeline timeline-padding">
            <div class="timeline-text">
              <h2>{{ timeline_section.elements[1].title }}</h2>
              <p>
                {{ timeline_section.elements[1].description }}
              </p>
            </div>
            <div class="timeline-img timeline-img-2">
              <span class="arrow_icon">
                <svg width="101" height="71">
                  <use href="#arrow_symbol" />
                </svg>
              </span>
              <img src="../assets/images/silene_dioica_seedling.svg" class="normal_img">
              <img src="../assets/images/silene_dioica_seedling_dark.svg" class="white_img">
            </div>
          </div>
          <!-- single-timeline -->
          <div class="single-timeline design-timeline">
            <div class="timeline-img timeline-img-3">
              <span class="arrow_icon">
                <svg width="101" height="71">
                  <use href="#arrow_symbol" />
                </svg>
              </span>
              <img src="../assets/images/silene_dioica_root.svg" class="normal_img" width="426.28122">
              <img src="../assets/images/silene_dioica_root_dark.svg" class="white_img" width="426.28122">
            </div>
            <div class="timeline-text">
              <h2>{{ timeline_section.elements[2].title }}</h2>
              <p>
                {{ timeline_section.elements[2].description }}
              </p>
            </div>
          </div>
          <!-- single-timeline -->
          <div class="single-timeline">
            <div class="timeline-text-implement">
              <h2>{{ timeline_section.elements[3].title }}</h2>
              <p>
                {{ timeline_section.elements[3].description }}
              </p>
              <div class="timeline-img timeline-img-4 d-md-none">
                <span class="arrow_icon">
                  <svg width="101" height="71">
                    <use href="#arrow_symbol" />
                  </svg>
                </span>
                <span class="arrow_icon">
                  <svg width="101" height="71">
                    <use href="#arrow_symbol" />
                  </svg>
                </span>
                <img src="../assets/images/silene_dioica_flower.svg" class="normal_img" width="384.37463"
                  height="821.5462">
                <img src="../assets/images/silene_dioica_flower_dark.svg" class="white_img" width="384.37463"
                  height="821.5462">
              </div>
              <div class="maintain-text">
                <h2>{{ timeline_section.elements[4].title }}</h2>
                <p>
                  {{ timeline_section.elements[4].description }}
                </p>
              </div>
            </div>
            <div class="timeline-img timeline-img-4 d-none d-md-block">
              <span class="arrow_icon">
                <svg width="101" height="71">
                  <use href="#arrow_symbol" />
                </svg>
              </span>
              <span class="arrow_icon">
                <svg width="101" height="71">
                  <use href="#arrow_symbol" />
                </svg>
              </span>
              <img src="../assets/images/silene_dioica_flower.svg" class="normal_img">
              <img src="../assets/images/silene_dioica_flower_dark.svg" class="white_img">
            </div>
          </div>
          <!-- single-timeline -->
          <div class="single-timeline exchange">
            <div class="timeline-img timeline-img-5">
              <img src="../assets/images/silene_dioica_capsule.svg" class="normal_img">
              <img src="../assets/images/silene_dioica_capsule_dark.svg" class="white_img">
              <span class="arrow_icon">
                <svg width="101" height="71">
                  <use href="#arrow_symbol" />
                </svg>
              </span>
            </div>
            <div class="timeline-text">
              <h2>{{ timeline_section.elements[5].title }}</h2>
              <p>
                {{ timeline_section.elements[5].description }}
              </p>
            </div>
          </div>
        </div>
      </section>
      <!-- Timeline Seaction End -->

      <!-- FAQ Section Start -->
      <section class="faq_section section pb-0">
        <div class="container">
          <div class="section_header text-center">
            <p class="tag">{{ faq_section.tag }}</p>
            <h2 class="title">{{ faq_section.title }}</h2>
            <p class="text">{{ faq_section.description }}</p>
          </div>
          <div class="accordion" id="accordionFaq">
            <div class="accordion-item">
              <h2 class="accordion-header" id="heading1">
                <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#collapse1"
                  aria-expanded="true" aria-controls="collapse1">
                  <div class="h3 mb-0">
                    {{ faq_section.questions[0].question }}
                  </div>
                  <span class="icon">
                    <svg width="16" height="16" version="2.0" class="bi bi-plus-circle">
                      <use href="#circle-arrow-plus" />
                    </svg>
                    <svg width="16" height="16" version="2.0" class="bi bi-dash-circle">
                      <use href="#circle-arrow-minus" />
                    </svg>
                  </span>
                </button>
              </h2>
              <div id="collapse1" class="accordion-collapse collapse show" aria-labelledby="heading1"
                data-bs-parent="#accordionFaq">
                <div class="accordion-body">
                  <p>
                    {{ faq_section.questions[0].answer }}
                  </p>
                </div>
              </div>
            </div>
            <div class="accordion-item">
              <h2 class="accordion-header" id="heading2">
                <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse"
                  data-bs-target="#collapse2" aria-expanded="false" aria-controls="collapse2">
                  <div class="h3 mb-0">
                    {{ faq_section.questions[1].question }}
                  </div>
                  <span class="icon">
                    <svg width="16" height="16" version="2.0" class="bi bi-plus-circle">
                      <use href="#circle-arrow-plus" />
                    </svg>
                    <svg width="16" height="16" version="2.0" class="bi bi-dash-circle">
                      <use href="#circle-arrow-minus" />
                    </svg>
                  </span>
                </button>
              </h2>
              <div id="collapse2" class="accordion-collapse collapse" aria-labelledby="heading2"
                data-bs-parent="#accordionFaq">
                <div class="accordion-body">
                  <p>
                    {{ faq_section.questions[1].answer }}
                  </p>
                </div>
              </div>
            </div>
            <div class="accordion-item">
              <h2 class="accordion-header" id="heading3">
                <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse"
                  data-bs-target="#collapse3" aria-expanded="false" aria-controls="collapse3">
