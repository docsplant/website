          <img src="../assets/images/flower_meadow_dark.svg" class="white_img">
        </div>
      </section>
      <!-- FAQ Area End -->

      <!-- Get Started Section Start -->
      <section class="get_started_section section">
        <div class="container">
          <h2 class="text-sm-center">
            {{ get_started_section.title }}
